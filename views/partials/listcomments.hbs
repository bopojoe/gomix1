<table class="ui selectable inverted table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Comment</th>
      
      <th></th>
    </tr>
  </thead>
  <tbody>
    {{#each feedback.comment}}
      <tr>
        <td>
          {{name}}
        </td>
        <td>
          {{comment}}
        </td>
        <td>
          <a href="/bookmark/{{../bookmark.id}}/deletelink/{{id}}" class="ui icon button">
            <i class="icon trash"></i>
          </a>
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>